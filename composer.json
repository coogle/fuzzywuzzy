{
    "name": "coogle/fuzzywuzzy",
    "description": "Fuzzy string matching based on FuzzyWuzzy from Seatgeek",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Michael Crumm",
        "email": "mike@crumm.net"
      }
    ],
    "require": {
        "php": "^8.1",
        "phpspec/php-diff": "^1.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.2",
        "vimeo/psalm": "^4.27"
    },
    "autoload": {
        "psr-4": {
          "FuzzyWuzzy\\": "lib/"
        }
    },
    "scripts": {
        "lint": "psalm --show-info=true",
        "test": "phpspec run"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
